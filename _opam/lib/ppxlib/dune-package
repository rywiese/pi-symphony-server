 (ppx.driver
  (flags (-corrected-suffix %{corrected-suffix} -diff-cmd - -dump-ast))
  (lint_flags (-corrected-suffix %{corrected-suffix} -diff-cmd - -null))
  (main Ppxlib.Driver.standalone)
  (replaces ocaml-migrate-parsetree)))
